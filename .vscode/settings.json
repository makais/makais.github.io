{
  "files.associations": {
    "*.html": "html",
    "*.md": "markdown",
    "*.toml": "toml"
  },
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}